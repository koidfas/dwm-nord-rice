# Brightnessctl
XF86MonBrightness{Up,Down}
	brightnessctl s 10{+,-}

# Lock Screen
super + x 
	systemctl suspend

#Volume Control
XF86AudioLowerVolume
	amixer set Master 5%-

XF86AudioRaiseVolume
	amixer set Master 5%+

XF86AudioMute
	amixer set Master toggle

# Open Rofi
super + Return
	rofi -show drun -show-icons

# PrintScreen
Print 
	xfce4-screenshooter

